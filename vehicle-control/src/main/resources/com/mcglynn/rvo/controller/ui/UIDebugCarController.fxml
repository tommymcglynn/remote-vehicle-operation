<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="1400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mcglynn.rvo.controller.ui.UIDebugCarController">
    <VBox alignment="TOP_CENTER" spacing="10.0" AnchorPane.bottomAnchor="636.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ImageView fx:id="imageView" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
        <HBox alignment="TOP_CENTER" spacing="10.0">
            <Label fx:id="leftLabel" text="Left" />
            <Label fx:id="rightLabel" text="Right" />
            <Label fx:id="upLabel" text="Up" />
            <Label fx:id="downLabel" text="Down" />
            <Label fx:id="throttleLabel" text="Throttle: 0" />
            <Label fx:id="brakeLabel" text="Brake: 0" />
            <Label fx:id="steerLabel" text="Steer: 0" />
            <Label fx:id="reverseLabel" text="Reverse: 0" />
            <Label fx:id="carLabel" text="Car:" />
        </HBox>
        <Button mnemonicParsing="false" onAction="#onStartClick" text="Start" />
    </VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
